CREATE TABLE PO_USER(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1)  NOT NULL PRIMARY KEY,VERSION INTEGER NOT NULL,PASSWORD VARCHAR(30) NOT NULL,EMAIL VARCHAR(80) NOT NULL,FIRSTNAME VARCHAR(50) NOT NULL,LASTNAME VARCHAR(50) NOT NULL,PHONENUMBER VARCHAR(50),ENABLED BOOLEAN,CREATEDDATE TIMESTAMP,CREATEDBY VARCHAR(50),UPDATEDBY VARCHAR(50),UPDATEDDATE TIMESTAMP,NAME VARCHAR(50) NOT NULL,DESCRIPTION VARCHAR(100),CONSTRAINT SYS_CT_67 UNIQUE(NAME))
CREATE TABLE PO_LAB(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1)  NOT NULL PRIMARY KEY,NAME VARCHAR(50) NOT NULL,DESCRIPTION VARCHAR(100),CONSTRAINT SYS_CT_69 UNIQUE(NAME))
CREATE TABLE PO_ITEM(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1)  NOT NULL PRIMARY KEY,VERSION INTEGER NOT NULL,NAME VARCHAR(50) NOT NULL,DESCRIPTION VARCHAR(100),ITEMNUMBER VARCHAR(50),DISTRIBUTOR VARCHAR(50),FORMAT VARCHAR(50),MANUFACTURER VARCHAR(50),PRODUCT_ID BIGINT NOT NULL,LISTPRICE_AMT DECIMAL(10,2),LISTPRICE_CURRENCY VARCHAR(3),CREATEDBY VARCHAR(50),CREATEDDATE TIMESTAMP,UPDATEDBY VARCHAR(50),UPDATEDDATE TIMESTAMP)
CREATE TABLE PO_ITEM_SUPPLIED(ITEM_SUPPLIED_ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1)  NOT NULL PRIMARY KEY,BIDINFO VARCHAR(100),BIDYEAR VARCHAR(4),SUPPLIER_ID BIGINT NOT NULL,ITEM_ID BIGINT NOT NULL,CREATEDDATE TIMESTAMP,CONSTRAINT FK_ITEMSUPPLIED_ITEM FOREIGN KEY(ITEM_ID) REFERENCES PO_ITEM(ID))
CREATE TABLE PO_SUPPLIER(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1)  NOT NULL PRIMARY KEY,CREATEDDATE TIMESTAMP,CREATEDBY VARCHAR(50),SALESREP_CONTACT VARCHAR(100),SALESREP_NAME VARCHAR(50),NAME VARCHAR(50) NOT NULL,DESCRIPTION VARCHAR(100),CONSTRAINT SYS_CT_71 UNIQUE(NAME))
CREATE TABLE PO_USER_ROLE(USER_ID BIGINT NOT NULL,ROLE VARCHAR(255),CONSTRAINT FKA85ECA2AF73AEE0F FOREIGN KEY(USER_ID) REFERENCES PO_USER(ID))
CREATE TABLE PO_CATEGORY(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1)  NOT NULL PRIMARY KEY,CREATEDBY VARCHAR(50),CREATEDDATE TIMESTAMP,NAME VARCHAR(50) NOT NULL,DESCRIPTION VARCHAR(100),CONSTRAINT SYS_CT_73 UNIQUE(NAME))
CREATE TABLE PO_HIST_STATUS(PO_ID BIGINT NOT NULL,STATUS_CODE VARCHAR(30),CREATEDBY VARCHAR(50),STATUSCHANGEDATE TIMESTAMP,STATUS_INDEX INTEGER NOT NULL,CONSTRAINT SYS_PK_PO_HIST_STATUS PRIMARY KEY(PO_ID,STATUS_INDEX))
CREATE TABLE PO_PURCHASE_ORDER(PO_ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1)  NOT NULL PRIMARY KEY,VERSION INTEGER NOT NULL,CONFIRMNUMBER VARCHAR(30),ESTIMATED_CAD_TOT DECIMAL(10,2),EFFECTIVE_CAD_TOT DECIMAL(10,2),EFFECTIVE_CAD_TAX DECIMAL(10,2),ORDERFOR VARCHAR(30),LAB_ID BIGINT NOT NULL,USER_ID BIGINT NOT NULL,PROJECT VARCHAR(100),PAYMENT_TYPE VARCHAR(50),CURRENT_STATUS VARCHAR(30),CURRENT_STATUS_DATE TIMESTAMP,OPENDATE TIMESTAMP,EST_RECEPTION_DATE TIMESTAMP,CONSTRAINT FK_PURCHASE_ORDER_LAB FOREIGN KEY(LAB_ID) REFERENCES PO_LAB(ID),CONSTRAINT FK_PURCHASE_ORDER_USER FOREIGN KEY(USER_ID) REFERENCES PO_USER(ID))
CREATE TABLE PO_PRODUCT(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1)  NOT NULL PRIMARY KEY,CATEGORY_ID BIGINT NOT NULL,CREATEDBY VARCHAR(50),CREATEDDATE TIMESTAMP,NAME VARCHAR(50) NOT NULL,DESCRIPTION VARCHAR(100),CONSTRAINT SYS_CT_75 UNIQUE(NAME),CONSTRAINT FK_PRODUCT_CATEGORY FOREIGN KEY(CATEGORY_ID) REFERENCES PO_CATEGORY(ID))
CREATE TABLE PO_USER_LAB(USER_ID BIGINT NOT NULL,LAB_ID BIGINT NOT NULL,CONSTRAINT SYS_PK_PO_USER_LAB PRIMARY KEY(USER_ID,LAB_ID),CONSTRAINT FK56E5159F73AEE0F FOREIGN KEY(USER_ID) REFERENCES PO_USER(ID),CONSTRAINT FK56E5159BDD0490D FOREIGN KEY(LAB_ID) REFERENCES PO_LAB(ID))
CREATE TABLE PO_LINEITEM(PO_ID BIGINT NOT NULL,ITEM_ID BIGINT NOT NULL,QUANTITY INTEGER NOT NULL,LISTPRICE_AMT DECIMAL(10,2),LISTPRICE_CURRENCY VARCHAR(3),SUPPLIER_ID BIGINT NOT NULL,LINENUM INTEGER NOT NULL,CONSTRAINT SYS_PK_PO_LINEITEM PRIMARY KEY(PO_ID,LINENUM),CONSTRAINT FK5C2E834765E2F5B FOREIGN KEY(PO_ID) REFERENCES PO_PURCHASE_ORDER(PO_ID),CONSTRAINT FK_LINEITEM_ITEM FOREIGN KEY(ITEM_ID) REFERENCES PO_ITEM(ID),CONSTRAINT FK_LINEITEM_SUPPLIER FOREIGN KEY(SUPPLIER_ID) REFERENCES PO_SUPPLIER(ID))
ALTER TABLE PO_ITEM ADD CONSTRAINT FK_ITEM_PRODUCT FOREIGN KEY(PRODUCT_ID) REFERENCES PO_PRODUCT(ID)
ALTER TABLE PO_ITEM_SUPPLIED ADD CONSTRAINT FK_ITEMSUPPLIED_SUPPLIER FOREIGN KEY(SUPPLIER_ID) REFERENCES PO_SUPPLIER(ID)
ALTER TABLE PO_HIST_STATUS ADD CONSTRAINT FKDED0696F65E2F5B FOREIGN KEY(PO_ID) REFERENCES PO_PURCHASE_ORDER(PO_ID)
